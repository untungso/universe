{
  "name": "@instead/universe",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "serve": "turbo run serve --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@instead/eslint-config": "workspace:0.0.0",
    "prettier": "^2.8.4",
    "turbo": "^1.7.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@7.12.2"
}
