{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false
    },
    "serve": {
      "cache": false
    }
  },
  "globalDependencies": [],
  "globalEnv": [
    "INSTEAD_TOKEN",
    "GITHUB_ID",
    "GITHUB_SECRET",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "NEXTAUTH_URL",
    "JWT_SECRET",
    "JWT_SIGNING_KEY",
    "JWT_ENCRYPTION_KEY",
    "APP_NAME",
    "NEXT_PUBLIC_SPACES_URL",
    "NEXT_PUBLIC_MICROS_URL",
    "NEXT_PUBLIC_FILAMENTS_URL",
    "NEXT_PUBLIC_ALCHEMY_ID"
  ]
}
